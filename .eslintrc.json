{
  "extends": [
    "eslint:recommended",
    "next/core-web-vitals",
    "eslint-config-prettier"
  ],
  "plugins": [
    "eslint-plugin-prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "no-console": [
      "error",
      {
        "allow": [
          "warn",
          "error"
        ]
      }
    ]
  }
}